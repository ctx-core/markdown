{
	"name": "@ctx-core/markdown",
	"version": "14.0.10",
	"description": "ctx-core markdown support",
	"keywords": [
		"ctx-core",
		"logger"
	],
	"homepage": "https://github.com/ctx-core/markdown#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/markdown/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/markdown.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.41",
		"@ctx-core/object": "workspace:^22.0.8",
		"@ctx-core/store": "workspace:^27.0.55",
		"@ctx-core/svelte": "workspace:^10.0.50",
		"marked": "^4.0.7",
		"svelte": "^3.44.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/express-serve-static-core": "^4.17.26",
		"@types/marked": "^4.0.1",
		"express-serve-static-core": "^0.1.1",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
