{
	"name": "@ctx-core/markdown",
	"version": "11.0.22",
	"description": "ctx-core markdown support",
	"keywords": [
		"ctx-core",
		"logger"
	],
	"homepage": "https://github.com/ctx-core/markdown#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/markdown/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/markdown/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^21.2.7",
		"@ctx-core/object": "workspace:^19.1.3",
		"@ctx-core/store": "workspace:^25.1.13",
		"@ctx-core/svelte": "workspace:^8.0.2",
		"marked": "^2.0.7",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^5.1.4",
		"@types/express-serve-static-core": "^4.17.21",
		"@types/marked": "^2.0.3",
		"express-serve-static-core": "^0.1.1",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
