{
	"name": "@ctx-core/markdown",
	"version": "14.0.1",
	"description": "ctx-core markdown support",
	"keywords": [
		"ctx-core",
		"logger"
	],
	"homepage": "https://github.com/ctx-core/markdown#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/markdown/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/markdown/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.33",
		"@ctx-core/object": "workspace:^22.0.0",
		"@ctx-core/store": "workspace:^27.0.46",
		"@ctx-core/svelte": "workspace:^10.0.41",
		"marked": "^4.0.7",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/express-serve-static-core": "^4.17.26",
		"@types/marked": "^4.0.1",
		"express-serve-static-core": "^0.1.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
