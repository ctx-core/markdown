{
	"name": "@ctx-core/markdown",
	"version": "13.0.58",
	"description": "ctx-core markdown support",
	"keywords": [
		"ctx-core",
		"logger"
	],
	"homepage": "https://github.com/ctx-core/markdown#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/markdown/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/markdown/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"generate_ctx_I_file": "swc $(./node_modules/.bin/generate_ctx_I_file | tail -n1) --out-dir dist --source-maps --config-file .swcrc"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.29",
		"@ctx-core/object": "workspace:^21.0.28",
		"@ctx-core/store": "workspace:^27.0.42",
		"@ctx-core/svelte": "workspace:^10.0.38",
		"marked": "^4.0.6",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.2.6",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.117",
		"@types/express-serve-static-core": "^4.17.26",
		"@types/marked": "^4.0.1",
		"express-serve-static-core": "^0.1.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}