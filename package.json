{
	"name": "@ctx-core/markdown",
	"version": "13.0.21",
	"description": "ctx-core markdown support",
	"keywords": [
		"ctx-core",
		"logger"
	],
	"homepage": "https://github.com/ctx-core/markdown#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/markdown/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/markdown/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.13",
		"@ctx-core/object": "workspace:^21.0.12",
		"@ctx-core/store": "workspace:^27.0.15",
		"@ctx-core/svelte": "workspace:^10.0.18",
		"esm": "^3.2.25",
		"marked": "^2.1.3",
		"svelte": "3.40.3"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.0.18",
		"@types/express-serve-static-core": "^4.17.24",
		"@types/marked": "^2.0.4",
		"express-serve-static-core": "^0.1.1",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}